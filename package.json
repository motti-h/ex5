{
  "name": "ex2",
  "version": "1.0.0",
  "description": "product store",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:run": "./node_modules/.bin/tsc",
    "build:clean": "rm -rf ./dist",
    "build": "npm run build:clean && npm run build:run",
    "serve": "node dist/index.js",
    "start": "npm run build && npm run watch && npm run serve",
    "lint:ts": "./node_modules/.bin/tslint --project . --format codeFrame",
    "lint:ts:fix": "npm -s run lint:ts -- --fix",
    "watch:ts": "./node_modules/.bin/tsc -w",
    "watch:js": "./node_modules/.bin/nodemon dist/index.js",
    "watch": "./node_modules/.bin/tsc-watch --onSuccess \"npm run watch:js\""
  },
  "author": "motti hadash",
  "license": "ISC",
  "devDependencies": {
    "@types/cors": "^2.8.5",
    "@types/express": "^4.16.1",
    "@types/node": "^12.0.2",
    "nodemon": "^1.19.0",
    "tsc-watch": "^2.2.1",
    "tslint": "^5.16.0",
    "typescript": "^3.4.5"
  },
  "dependencies": {
    "@types/express-winston": "^3.0.3",
    "@types/joi": "^14.3.3",
    "@types/jsonwebtoken": "^8.3.2",
    "@types/passport": "^1.0.0",
    "@types/passport-jwt": "^3.0.1",
    "@types/passport-local": "^1.0.33",
    "@types/winston": "^2.4.4",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-winston": "^3.1.0",
    "joi": "^14.3.1",
    "jsonwebtoken": "^8.5.1",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "save": "^2.4.0",
    "winston": "^3.2.1"
  }
}
